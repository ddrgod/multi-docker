{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "alyhdr/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "alyhdr/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "alyhdr/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "alyhdr/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links": ["client","server"]
        }
    ]
}